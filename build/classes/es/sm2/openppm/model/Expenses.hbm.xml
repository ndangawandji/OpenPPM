<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Hibernate Generator by Javier Hernandez -->
<hibernate-mapping>
    <class name="es.sm2.openppm.model.Expenses" table="expenses">
        <id name="idExpense" type="java.lang.Integer">
            <column name="IdExpense" />
            <generator class="native">
                <param name="sequence">expenses_SEQ</param>
            </generator>
        </id>
        <many-to-one name="projectcosts" class="es.sm2.openppm.model.Projectcosts" fetch="select">
            <column name="IdProjectCosts" />
        </many-to-one>
        <many-to-one name="budgetaccounts" class="es.sm2.openppm.model.Budgetaccounts" fetch="select">
            <column name="IdBudgetAccount" />
        </many-to-one>
        <property name="planned" type="java.lang.Double">
            <column name="Planned" precision="22" scale="0" />
        </property>
        <property name="actual" type="java.lang.Double">
            <column name="Actual" precision="22" scale="0" />
        </property>
        <property name="description" type="string">
            <column name="Description" length="50" />
        </property>
        <set name="expensesheets" inverse="true" lazy="true" table="expensesheet" fetch="select">
            <key>
                <column name="idExpense" />
            </key>
            <one-to-many class="es.sm2.openppm.model.Expensesheet" />
        </set>
    </class>
</hibernate-mapping>
